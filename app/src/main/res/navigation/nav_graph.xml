<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/indexFragment">

    <fragment
        android:id="@+id/newsFeedFragment"
        android:name="com.example.secondchance.newsFeedFragment"
        android:label="News Feed"
        tools:layout="@layout/fragment_news_feed">
        <action
            android:id="@+id/action_newsFeedFragment_to_addPostFragment"
            app:destination="@id/addPostFragment" />
        <action
            android:id="@+id/action_newsFeedFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_newsFeedFragment_to_inboxFragment"
            app:destination="@id/messagesFragment" />
        <action
            android:id="@+id/action_newsFeedFragment_self"
            app:destination="@id/newsFeedFragment" />
        <action
            android:id="@+id/action_newsFeed_to_item_details"
            app:destination="@id/ItemDetailsFragment" />
        <action
            android:id="@+id/action_newsFeed_to_maps"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_newsFeedFragment_to_anotherProfileFragment"
            app:destination="@id/anotherProfileFragment" />
        <action
            android:id="@+id/action_newsFeedFragment_to_singlePostFragment"
            app:destination="@id/singlePostFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.secondchance.profileFragment"
        android:label="My Profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_addPostFragment"
            app:destination="@id/addPostFragment" />
        <action
            android:id="@+id/action_profileFragment_to_newsFeedFragment"
            app:destination="@id/newsFeedFragment" />
        <action
            android:id="@+id/action_profileFragment_self"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_profile_to_item_details"
            app:destination="@id/ItemDetailsFragment" />
        <action
            android:id="@+id/action_profileFragment_pop"
            app:popUpTo="@id/newsFeedFragment"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_profileFragment_to_messagesFragment"
            app:destination="@id/messagesFragment" />
        <argument
            android:name="userID"
            app:argType="string" />
        <action
            android:id="@+id/action_profile_to_editProfile"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_profile_to_editItem"
            app:destination="@id/editItemFragment" />
        <action
            android:id="@+id/action_profileFragment_to_singlePostFragment"
            app:destination="@id/singlePostFragment" />
    </fragment>
    <fragment
        android:id="@+id/addPostFragment"
        android:name="com.example.secondchance.addPostFragment"
        android:label="Add A Post"
        tools:layout="@layout/fragment_add_post">
        <argument android:name="userID" />
        <action
            android:id="@+id/action_addPostFragment_pop"
            app:popUpTo="@id/addPostFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/messagesFragment"
        android:name="com.example.secondchance.messagesFragment"
        android:label="Messages"
        tools:layout="@layout/fragment_messages">
        <argument android:name="userID" />
        <action
            android:id="@+id/action_messages_to_private_messages"
            app:destination="@id/PrivateMsgFragment" />
    </fragment>
    <fragment
        android:id="@+id/ItemDetailsFragment"
        android:name="com.example.secondchance.itemDetailsFragment"
        android:label="fragment_item_details"
        tools:layout="@layout/fragment_item_details" >
        <argument
            android:name="item_id"
            app:argType="string" />
        <argument
            android:name="item_name"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/PrivateMsgFragment"
        android:name="com.example.secondchance.privateMessagesFragment"
        android:label="fragment_private_messages"
        tools:layout="@layout/fragment_private_messages" >
        <action
            android:id="@+id/action_private_messages_pop"
            app:popUpTo="@id/messagesFragment"
            app:popUpToInclusive="false" />
        <argument
            android:name="message_id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.secondchance.editProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" >
        <action
            android:id="@+id/action_editProfilePop"
            app:popUpToInclusive="false" />
        <argument
            android:name="id_edit_profile"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/editItemFragment"
        android:name="com.example.secondchance.EditItemFragment"
        android:label="fragment_edit_item"
        tools:layout="@layout/fragment_edit_item" >
        <argument
            android:name="item_id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/indexFragment"
        android:name="com.example.secondchance.IndexFragment"
        android:label="fragment_index"
        tools:layout="@layout/fragment_index" >
        <action
            android:id="@+id/action_index_to_register"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_index_to_login"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.secondchance.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_register_to_login"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.secondchance.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_register"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_login_to_newsFeed"
            app:destination="@id/newsFeedFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.secondchance.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" >
        <action
            android:id="@+id/action_maps_to_ItemDetails"
            app:destination="@id/ItemDetailsFragment" />
        <action
            android:id="@+id/action_mapsFragment_to_newsFeedFragment"
            app:destination="@id/newsFeedFragment" />
        <action
            android:id="@+id/action_mapsFragment_pop"
            app:popUpTo="@id/mapsFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/anotherProfileFragment"
        android:name="com.example.secondchance.anotherProfileFragment"
        android:label="fragment_another_profile"
        tools:layout="@layout/fragment_another_profile" >
        <action
            android:id="@+id/action_anotherProfileFragment_pop"
            app:popUpTo="@id/anotherProfileFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/singlePostFragment"
        android:name="com.example.secondchance.singlePostFragment"
        android:label="fragment_single_post"
        tools:layout="@layout/fragment_single_post" >
        <argument
            android:name="PostId"
            app:argType="string" />
        <action
            android:id="@+id/action_singlePostFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_singlePostFragment_to_editItemFragment"
            app:destination="@id/editItemFragment" />
    </fragment>
</navigation>