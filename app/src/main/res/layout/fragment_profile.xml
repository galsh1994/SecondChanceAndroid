<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#74F6F9FA"
    tools:context=".newsFeedFragment">


    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="24dp"
        android:background="#FBFCFD"
        android:fontFamily="@font/quicksand"
        android:text="Profile"
        android:textColor="#07080C"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
//bottom nav
    <Button
        android:id="@+id/addApost_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="50dp"
        android:background="#FDF7F7"
        android:fontFamily="@font/quicksand"
        android:textColor="#07080C"
        android:textSize="24sp"
        app:backgroundTint="#45FFFFFF"
        app:icon="@drawable/plus"
        app:iconGravity="textEnd"
        app:iconPadding="0dp"
        app:iconSize="40dp"
        app:iconTint="#615D5D"
        app:iconTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/messagesFrom_profile" />

    <Button
        android:id="@+id/visitProfileFrom_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="50dp"
        android:background="#FDF7F7"
        android:fontFamily="@font/quicksand"
        android:textColor="#07080C"
        android:textSize="18sp"
        app:backgroundTint="#45FFFFFF"
        app:icon="@drawable/profile"
        app:iconGravity="textEnd"
        app:iconSize="40dp"
        app:iconTint="#656363"
        app:iconTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/addApost_profile"
        app:layout_constraintStart_toEndOf="@+id/homeFrom_profile" />

    <Button
        android:id="@+id/messagesFrom_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="50dp"
        android:background="#FDF7F7"
        android:fontFamily="@font/quicksand"
        android:textColor="#07080C"
        android:textSize="18sp"
        app:backgroundTint="#45FFFFFF"
        app:icon="@drawable/inbox"
        app:iconGravity="textEnd"
        app:iconSize="40dp"
        app:iconTint="#656363"
        app:iconTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/homeFrom_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="50dp"
        android:background="#FDF7F7"
        android:fontFamily="@font/quicksand"
        android:textColor="#07080C"
        android:textSize="18sp"
        app:backgroundTint="#45FFFFFF"
        app:icon="@drawable/home"
        app:iconGravity="textEnd"
        app:iconSize="40dp"
        app:iconTint="#656363"
        app:iconTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/itemDetailsFrom_profile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="110dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.408"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:src="@tools:sample/avatars" />

    //bottom nav ends here

</androidx.constraintlayout.widget.ConstraintLayout>